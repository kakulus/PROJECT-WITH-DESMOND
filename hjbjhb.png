  background-color: white;
}
.section3 h2 {
  font-size: 28px;
  font-family: "Poppins", sans-serif;
  color: #333333;
  margin: 0px 3px 25px 4px;
}
.section3 .swiper-slide img {
  width: 350px;
  border-radius: 13px;
}
.section3 .swiper-slide h3 {
  font-size: 28px;
  font-family: sans-serif, "poppins";
  margin: 0px 0px 16px;
  color: #333333;
  text-align: left;
}
.section3 .swiper-slide p {
  font-size: 19px;
  margin: 0px 0px 32px;
  color: #6b747b;
  margin: 7px 9px 20px 10px;
  text-align: left;
  font-family: sans-serif, "Open sans";
}
.section3 .swiper-slide .TB {
  font-size: 16px;
  padding: 24px;
  background-color: #f4f9ff;
  width: 350px;
  margin-left: 25px;
  border-radius: 10px;
}
.section3 .swiper-slide .TT {
  font-size: 16px;
