@import url("https://fonts.googleapis.com/css2?family=Open+Sans:wght@300&display=swap");
* {
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  list-style: none;
  text-decoration: none;
}
body {
  background-color: white;
}
nav {
  display: inline;
  height: 50px;
  width: 100%;
  background-color: white;
  position: fixed;
  top: 0;
  z-index: 9999;
  overflow: hidden;
}
nav ul {
  float: right;
  margin-right: 20px;
  margin-bottom: 30px;
}
nav ul li {
  display: inline-block;
  line-height: 55px;
  margin: 0 5px;
}
nav ul li a {
  color: #0055eb;
  font-size: 14px;
  font-family: "open sans", sans-serif;
  text-transform: uppercase;
  border-radius: 3px;
  padding: 0px 4px;
}
.img1 img {
  margin-top: 10px;
  margin-left: 50px;
  height: 40px;
  width: 40px;
}
nav .logo {
  position: relative;
  bottom: 20%;
  margin-right: 10px;
  font-size: 30px;
  font-weight: 900;
}
.dropdown {
  display: inline-block;
  position: relative;
}
.dropdown-content {
  display: none;
  position: absolute;
  width: 100%;
  overflow: auto;
}
.dropdown:hover .dropdown-content {
  display: block;
}
.dropdown-content a {
  display: block;
  color: gray;
  padding: 4px;
  text-decoration: none;
}
.dropdown-content a:hover {
  color: blue;
}
.checkbtn {
  display: none;
  font-size: 35px;
  color: black;
  float: right;
  line-height: 70px;
  margin-right: 40px;
  cursor: pointer;
}
#check {
  display: none;
}
.header {
  background: #f4f9ff;
  margin-top: 70px;
  color: white;
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  align-items: center;
  font-family: sans-serif, "Open sans";
}
.header .header-write {
  text-align: left;
  width: 55%;
}
.header .header-write h2 {
  font-size: 36px;
  padding: 20px;
  color: #333;
  font-family: "Open sans", sans-serif;
}
.header .header-write p {
  font-size: 20px;
  font-weight: 400;
  color: #6b747b;
  margin-bottom: 40px;
  font-family: "Poppins", sans-serif;
  margin-left: 25px;
}
.header .h1 {
  text-align: center;
  color: white;
  padding: 15px 30px 15px 30px;
  font-size: 16px;
  font-weight: 600;
  border-radius: 20px;
  background-color: rgb(74, 74, 208);
  margin-right: 30px;
  margin-left: 150px;
}
.header .h1:hover {
  border: 1px solid rgb(74, 74, 208);
  color: rgb(74, 74, 208);
  background-color: white;
}
.header .h2 {
  color: rgb(17, 16, 16);
  padding: 15px 30px 15px 30px;
  font-size: 16px;
  font-weight: 600;
  border-radius: 20px;
  border: 1px solid rgb(17, 16, 16);
}
.header .h2:hover {
  color: white;
  background-color: black;
}
.header img {
  margin-top: 30px;
  width: 600px;
}
.frame-decoration {
  width: 100%;
  height: 44px;
}
.section1 {
  background: white;
  text-align: center;
  margin-top: 80px;
}
.section1 h2 {
  color: #333333;
  font-family: "poppins", sans-serif;
  font-size: 32px;
}
.section1 .cards {
  margin-top: 50px;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.section1 .card {
  padding: 10px 25px 15px 25px;
}
.section1 .card i {
  padding: 65px 0px 0px 0px;
  color: blue;
  height: 130px;
  width: 130px;
  background-color: rgb(217, 236, 243);
  border-radius: 50%;
  margin-bottom: 10px;
  font-size: 60px;
}
.section1 .card h5 {
  margin-bottom: 15px;
  color: #333333;
  font-size: 20px;
  font-family: sans-serif, "poppins";
  margin: 0px 0px 8px;
}
.section1 .card p {
  font-size: 16px;
  color: #6b747b;
  font-family: "Open sans", sans-serif;
}

.slideshow-container {
  max-width: 1000px;
  position: relative;
  margin: auto;
}
.mySlides {
  display: flex;
  flex-direction: row;
  padding: 0px 24px;
}
.beflx {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}
.mySlides img {
  align-items: center;
  width: 350px;
  margin-left: 150px;
}
.text {
  align-self: flex-end;
  text-align: left;
  margin-left: 50px;
}
.text h3 {
  font-size: 28px;
  font-family: sans-serif, "Poppins";
  color: #333333;
  margin: 15px 3px 15px 3px;
}
.text p {
  font-size: 16px;
  font-family: sans-serif, "Open sans";
  margin: 15px 3px 15px 3px;
  color: #6b747b;
}
.text ul li {
  margin: 0px 0px 24px;
  padding: 0px 12px 0px 0px;
}
.text ul li i {
  padding: 3px 5px;
}
.tabs {
  margin-top: 150px;
}
.tabs ul {
  display: flex;
  padding: 10px 20px;
  text-align: center;
  justify-content: center;
}
.tabs ul li {
  padding: 7px 7px;
}
.tabs ul li a {
  margin: 0px 0px 16px;
  padding: 8px 16px 4px;
  font-size: 24px;
  font-family: sans-serif, "poppins";
  font-weight: 600;
  border-bottom: 3px solid #c9d9ec;
  transition: all 0.6s ease;
  color: #afbbc9;
}
.tabs li a#active,
.tabs li a:hover {
  border-bottom: 3px solid #ed832b;
  background-color: transparent;
  color: #333333;
}
.fade {
  animation-name: fade;
  animation-duration: 1.5s;
}
@keyframes fade {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
.btn-gb {
  padding: 15px 40px 15px 40px;
  color: white;
  background-color: blue;
  border-radius: 25px;
  font-size: 16px;
  font-weight: 600;
}
.btn-gb:hover {
  border: 1px solid blue;
  color: blue;
  background-color: white;
}
.space6 {
  margin-top: 60px;
}
.section2 {
  background: #f4f9ff;
  text-align: center;
}
.section2 h2 {
  font-weight: 519;
  font-size: 40px;
  font-family: sans-serif, "poppins";
}
.section2 .vdp img {
  align-items: center;
  width: 550px;
}
.section2 .vdp i {
  position: relative;
  bottom: 180px;
  left: 300px;
  background-color: orange;
  color: white;
  padding: 30px 0px 0px 5px;
  border-radius: 50%;
  height: 60px;
  width: 60px;
}
.section2 .vdp {
  text-align: center;
